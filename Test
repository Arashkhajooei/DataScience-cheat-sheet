      "source": [
        "import time\n",
        "from googlesearch import search\n",
        "\n",
        "query = \"ibm\"\n",
        "num_results = 20\n",
        "batch_size = 5\n",
        "pause_between_batches = 5  # Pause in seconds between batches (adjust if needed)\n",
        "\n",
        "results = []\n",
        "\n",
        "# Perform the search in batches\n",
        "for i in range(0, num_results, batch_size):\n",
        "    # Set the number of results\n",
        "    num = min(batch_size, num_results - i)\n",
        "\n",
        "    # Perform the search and store the results\n",
        "    try:\n",
        "        search_results = search(query, num=num, pause=pause_between_batches)\n",
        "        results.extend(search_results)\n",
        "    except Exception as e:\n",
        "        print(f\"An error occurred: {e}\")\n",
        "    \n",
        "    # Delay between batches to avoid \"Too Many Requests\" error\n",
        "    time.sleep(pause_between_batches)\n",
        "\n",
        "# Print the search results\n",
        "for result in results:\n",
        "    print(result)"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "from googlesearch import search\n",
        "import time\n",
        "\n",
        "def fetch_search_results(query, num_results=100, batch_size=5, pause_time=5):\n",
        "    total_results = set()  # Use a set to store unique results\n",
        "    num_batches = num_results // batch_size\n",
        "\n",
        "    for batch in range(num_batches):\n",
        "        if len(total_results) >= num_results:\n",
        "            break  # Stop fetching more results if the desired count is reached\n",
        "\n",
        "        start_index = batch * batch_size\n",
        "        end_index = start_index + batch_size\n",
        "\n",
        "        batch_results = list(search(query, num=batch_size, start=start_index, pause=pause_time))\n",
        "\n",
        "        total_results.update(batch_results)  # Add batch results to the set\n",
        "\n",
        "        time.sleep(pause_time)  # Pause between batches\n",
        "\n",
        "    return list(total_results)[:num_results]  # Convert set back to a list and limit the number of results\n",
        "\n",
        "# Example usage\n",
        "query = \"ibm\"\n",
        "results = fetch_search_results(query, num_results=100, batch_size=5, pause_time=1)\n",
        "\n",
        "# Print the search results\n",
        "for result in results:\n",
        "    print(result)"
      ],
      "metadata": {
        "id": "lJv4AoEPInc7"
